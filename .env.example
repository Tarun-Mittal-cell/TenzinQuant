# API Keys (Optional - using free alternatives by default)
OPENAI_API_KEY=your_openai_api_key_here  # Optional: Only needed if using OpenAI features
ALPHA_VANTAGE_KEY=your_alpha_vantage_key_here  # Optional: Only needed for extended market data
FINNHUB_KEY=your_finnhub_key_here  # Optional: Only needed for real-time websocket data

# Cache Settings
CACHE_DURATION=300  # Default cache duration in seconds
CACHE_STRATEGY=aggressive  # Options: conservative, moderate, aggressive

# Performance Settings
USE_GPU=true  # Set to false if GPU is not available
NUM_WORKERS=4  # Number of worker threads for parallel processing
BATCH_SIZE=32  # Batch size for model inference

# Logging
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FILE=quantum_trader.log

# UI Settings
THEME=dark  # Options: dark, light
UPDATE_INTERVAL=1000  # UI update interval in milliseconds

# Model Settings
DEFAULT_CONFIDENCE_THRESHOLD=0.95  # Minimum confidence for predictions
ENSEMBLE_WEIGHTS=0.3,0.3,0.2,0.2  # Weights for LSTM,Transformer,XGBoost,LightGBM

# Security (Optional)
JWT_SECRET_KEY=your_secret_key_here  # Only needed if using authentication
ENCRYPTION_KEY=your_encryption_key_here  # Only needed if storing sensitive data